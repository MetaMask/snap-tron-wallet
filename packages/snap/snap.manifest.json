{
  "version": "1.17.0",
  "description": "Manage Tron using MetaMask",
  "proposedName": "Tron",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snap-tron-wallet.git"
  },
  "source": {
    "shasum": "4cKUrTAfxh59rVmUHtKjA8A+A9Gk1L3UnItPe15JZ9w=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "@metamask/tron-wallet-snap",
        "registry": "https://registry.npmjs.org/"
      }
    },
    "locales": ["locales/en.json"]
  },
  "initialConnections": {
    "https://portfolio.metamask.io": {}
  },
  "initialPermissions": {
    "endowment:keyring": {
      "allowedOrigins": ["https://portfolio.metamask.io"]
    },
    "snap_getBip32Entropy": [
      {
        "path": ["m", "44'", "195'"],
        "curve": "secp256k1"
      }
    ],
    "endowment:network-access": {},
    "snap_manageAccounts": {},
    "snap_manageState": {},
    "snap_dialog": {},
    "snap_getPreferences": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "duration": "PT30S",
          "request": {
            "method": "onSynchronizeSelectedAccountsCronjob"
          }
        }
      ]
    },
    "endowment:assets": {
      "scopes": ["tron:728126428"]
    }
  },
  "platformVersion": "9.3.0",
  "manifestVersion": "0.1"
}
